# This is the port our server will be running on
PORT=5000

# This is the URL the server will use for the database. The example is an Atlas URL, but can be anything
MONGODB_URL=mongodb+srv://username:password@cluster.blablabla.mongodb.net/?retryWrites=true&w=majority

# This is the secret key used to sign the JWT tokens
JWT_SECRET=some_secret

# This is the URL the server will use for the frontend. This is used to set the CORS policy
ORIGIN=http://localhost:5173